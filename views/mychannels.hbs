<h1>MY CHANNELS!</h1>
<div id="main-content-channels">

</div>

<script rel='text/javascript' src='/javascripts/mychannelsT.js' />
</script>

<script id="channel-template" type="text/x-handlebars-template">
    <div id="title-container">
        <h1>\{{ nombre }}</h1>
        <form action="/addvideos/\{{id}}"><button class="btn btn-lg btn-primary btn-block" type="submit">Add videos</button>
</form>
    </div>
    <div class="channel-container" id="\{{id}}">
        
        
    </div>
</script>

<script id="video-template" type="text/x-handlebars-template">
    <div id="video-container">
        <iframe width="560" height="315" src="\{{url}}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
        allowfullscreen></iframe>
        <a href="/videos/\{{ id }}">\{{ title }}</a>
        <h4>\{{user}}</h4>
    </div>
</script>